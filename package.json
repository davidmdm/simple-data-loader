{
  "name": "simple-data-loader",
  "version": "0.5.1",
  "description": "Caches function results by key for specified time or indefintiy",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test/dataloader.spec.js",
    "coverage": "nyc mocha ./test/dataloader.spec.js"
  },
  "keywords": [
    "cache",
    "promise",
    "promise-cache",
    "loader"
  ],
  "author": "David Desmarais-Michaud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidmdm/simple-data-loader"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "node_modules"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
